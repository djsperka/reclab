                result% := GenTrialsDis%(recExpAMDiscButton%);  'Generate trials for AMDis   '*jao
                'message("AM DISC is not yet implemented!|I GUESS YOU WERE FOOLED!");  'debug placeholder
                
            case recExpToRun% = RecExpSpaceDisButton% then
